{
  "name": "partial-json-imperva",
  "version": "0.1.7",
  "description": "IMPERVA: Parse partial JSON generated by LLM",
  "keywords": [
    "JSON",
    "parser",
    "LLM",
    "nlp"
  ],
  "homepage": "https://promplate.dev/partial-json-parser",
  "repository": "https://github.com/isaac-jaynes-imperva/partial-json-parser-js-esm",
  "license": "MIT",
  "author": "Muspi Merol <me@promplate.dev>",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    },
    "./options": {
      "types": "./build/options.d.ts",
      "default": "./build/options.js"
    }
  },
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "prettier": "^3.3.3",
    "typescript": "^5.7.3",
    "vitest": "^1.2.2"
  }
}
